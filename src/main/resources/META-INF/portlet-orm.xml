<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.liferay.symposium.portlet.report.model.impl.ReportModelImpl">
		<attributes>
			<id name="reportId" />
			<basic name="uuid">
				<column name="UUID_" />
			</basic>
			<basic name="groupId" />
			<basic name="companyId" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="reportName" />
			<basic name="reportStatus" />
			<basic name="startTime">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="endTime">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="sectionSummary" />
			<basic name="sectionSummaryStatus" />
			<basic name="sectionCosts" />
			<basic name="sectionCostsStatus" />
			<basic name="sectionROI" />
			<basic name="sectionROIStatus" />
			<basic name="sectionMetrics" />
			<basic name="sectionMetricsStatus" />
			<basic name="sectionRecommendations" />
			<basic name="sectionRecommendationsStatus" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<entity class="com.liferay.symposium.portlet.report.model.impl.ReportImpl" name="Report">
		<table name="SYMPOSIUM_Report" />
		<attributes />
	</entity>
</entity-mappings>